[alias]
  c = commit
  ca = commit --amend
	co = checkout
  cb = checkout -b
  b = branch
  ba = branch -a
  dc = diff --cached
  unstage = reset HEAD
  l = log --graph --abbrev-commit --decorate --date=relative --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all
  f = fetch
  rb = rebase
[core]
	editor = emacs
	excludesfile = /Users/tyamagu2/.global_gitignore
[push]
	default = simple
[user]
	name = tyamagu2
  email = tyamagu2@gmail.com
